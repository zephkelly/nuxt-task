{
    "name": "nuxt-task",
    "version": "1.1.0",
    "description": "A reliable cron job library with nitro task integration",
    "repository": "https://github.com/zephkelly/nuxt-task",
    "license": "MIT",
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/types.d.ts",
            "import": "./dist/module.mjs"
        },
        "./runtime/*": "./dist/runtime/*",
        "./handler": {
            "types": "./dist/runtime/server/task/handler.d.ts",
            "import": "./dist/runtime/server/task/handler.js"
        }
    },
    "typesVersions": {
        "*": {
            "*": [
                "./dist/types.d.ts"
            ]
        }
    },
    "main": "./dist/module.mjs",
    "types": "./dist/types.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "prepack": "nuxt-module-build build",
        "dev": "nuxi dev playground",
        "dev:build": "nuxi build playground",
        "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
        "release": "npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
        "lint": "eslint . --fix",
        "lint:fix": "eslint . --fix",
        "test": "vitest run",
        "test:watch": "vitest watch",
        "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit",
        "docs:dev": "vitepress dev docs",
        "docs:build": "vitepress build docs",
        "docs:preview": "vitepress preview docs"
    },
    "dependencies": {
        "@nuxt/kit": "^3.19.2",
        "defu": "^6.1.4",
        "luxon": "^3.7.2",
        "pathe": "^2.0.3"
    },
    "devDependencies": {
        "@nuxt/devtools": "^1.7.0",
        "@nuxt/eslint-config": "^0.7.6",
        "@nuxt/module-builder": "^0.8.4",
        "@nuxt/schema": "^3.19.2",
        "@nuxt/test-utils": "^3.19.2",
        "@rollup/plugin-node-resolve": "^16.0.1",
        "@rollup/plugin-typescript": "^12.1.4",
        "@types/luxon": "^3.7.1",
        "@types/node": "latest",
        "@vitest/coverage-v8": "^3.2.4",
        "@vue/test-utils": "^2.4.6",
        "changelogen": "^0.5.7",
        "eslint": "^9.36.0",
        "happy-dom": "^16.8.1",
        "nuxt": "^3.19.2",
        "playwright-core": "^1.55.1",
        "redis": "^4.7.1",
        "rollup": "^4.52.3",
        "rollup-plugin-esbuild": "^6.2.1",
        "tslib": "^2.8.1",
        "typescript": "~5.7.3",
        "vitepress": "^1.6.4",
        "vitest": "^3.2.4",
        "vue-tsc": "^2.2.12"
    },
    "peerDependencies": {
        "redis": "^4.7.0"
    },
    "peerDependenciesMeta": {
        "redis": {
            "optional": true
        }
    }
}
